var result = '   ';
tableSize = 10
var longest = (tableSize*tableSize).toString().length
for (var i = 0; i <= tableSize; i++) {

    for (var j = 0; j <= tableSize; j++) {

        if(i>0 && j>0){
        result += (i*j).toString().padStart(longest, " ") + ' ';
        }
    }
    result += '\n'
}

console.log(result);
